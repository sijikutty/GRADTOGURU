    page.goto(Drill_link)
    #page.set_timeout(60000)
    with page.expect_download() as download_info1:
        #page.set_timeout(60000)
        page.frame_locator("iframe[name=\"TargetContent\"]").locator("[id=\"NVS_WRK_RUN_DRILL\\$0\"]").click()

    download = download_info1.value
    path=r"Result/"+ download.suggested_filename
    download.save_as(path=r"Result/"+ download.suggested_filename)
