    targetContentFrame.get_by_role("link", name="Details").click()
    page.frame_locator("iframe[name=\"ptModFrame_1\"]").get_by_role("link", name="View Log/Trace").click()
    with page.expect_download() as download_info:
        with page.expect_popup() as page1_info:
            page.frame_locator("iframe[name=\"ptModFrame_1\"]").get_by_role("link", name="BLC.csv").click()

