from openpyxl import load_workbook

# Load the Excel file
workbook = load_workbook('example.xlsx')
worksheet = workbook.active

# Function to check if outlining (grouping) exists in rows
def check_row_grouping():
    for row in worksheet.iter_rows():
        for cell in row:
            if cell.row_outline_level != 0:
                return True
    return False

# Function to check if outlining (grouping) exists in columns
def check_column_grouping():
    for column in worksheet.iter_cols():
        for cell in column:
            if cell.column_outline_level != 0:
                return True
    return False

# Check if outlining (grouping) exists in rows
if check_row_grouping():
    print("Row grouping exists in the Excel file")
else:
    print("Row grouping does not exist in the Excel file")

# Check if outlining (grouping) exists in columns
if check_column_grouping():
    print("Column grouping exists in the Excel file")
else:
    print("Column grouping does not exist in the Excel file")
