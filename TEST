Dim WshShell
Set WshShell = CreateObject("WScript.Shell")

' Define the path to the Python interpreter
Dim pythonExe
pythonExe = "C:\path\to\python.exe"

' Define the path to the Python script
Dim pythonScriptPath
pythonScriptPath = "C:\path\to\vv.py"

' Define the function name to call
Dim functionName
functionName = "testcase"

' Define any arguments to pass to the function
Dim functionArgs
functionArgs = "arg1 arg2"

' Construct the command to execute
Dim command
command = pythonExe & " " & pythonScriptPath & " " & functionName & " " & functionArgs

' Execute the command
Dim objExec
Set objExec = WshShell.Exec(command)

' Wait for the command to finish executing
Do While objExec.Status = 0
    WScript.Sleep 100
Loop

' Retrieve and display the command output
Dim output
output = objExec.StdOut.ReadAll()
MsgBox output
