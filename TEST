from pywinauto import Application

# Launch the application
app = Application().start("your_application.exe")

# Get the main window
main_window = app.window()

# Print the text of the main window
print("Main window text:", main_window.text())

# Get all child windows
child_windows = main_window.children()

# Iterate through child windows and print their texts
for child_window in child_windows:
    print("Child window text:", child_window.window_text())
