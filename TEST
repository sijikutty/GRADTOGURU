    if iframe_index is not None:
        try:
            # Define the selector for the 'View Log/Trace' link
            view_link_selector = "div[id='win0divPMN_DERIVED_INDEX_BTN'] span[title='Index Page'] a"

            # Fetch the outer HTML of the element within the iframe
            outer_html = frame.evaluate(f"""
                var element = document.querySelector('{view_link_selector}');
                return element ? element.outerHTML : null;
            """)

            # Print the outer HTML for debugging
            print(f"View Link Outer HTML: {outer_html}")

            # Additional actions if needed, e.g., clicking the link
            if outer_html:
                # Check if the element is visible and then perform actions
                is_visible = frame.evaluate(f"""
                    var element = document.querySelector('{view_link_selector}');
                    return element && window.getComputedStyle(element).display !== 'none' && window.getComputedStyle(element).visibility !== 'hidden';
                """)
                print(f"Element visible: {is_visible}")

                if is_visible:
                    # Scroll into view and click the element
