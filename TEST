    page.frame_locator("iframe[name=\"ptModFrame_0\"]").get_by_role("button", name="Run").click()
    page.get_by_role("button", name="Yes").click()
    page.frame_locator("iframe[name=\"ptModFrame_3\"]").get_by_label("Server Name").select_option("PSUNX")
    page.frame_locator("iframe[name=\"ptModFrame_3\"]").get_by_role("button", name="OK", exact=True).click()
