import boto3 
from botocore.exceptions import ClientError
import sys

def upload_file(temp_file, bucket, s3_filename):
    try:
        session = boto3.Session(profile_name='dev')
        s3_client = session.client('s3')

        response = s3_client.upload_file(temp_file, bucket, s3_filename)
        print(response)

    except ClientError as e:
        print(e)
        return False
    
    return True

if __name__ == '__main__':
    # Getting command line arguments
    temp_file = "C:/VV/tempFile.json"  # Path to your existing tempfile
    s3bucket = sys.argv[1]
    s3_pathname = sys.argv[2]

    response = upload_file(temp_file, s3bucket, s3_pathname)
    print(response)
