Function ReadJsonFromFile(filePath)
    Dim jsonString
    jsonString = JSONUtil.Parse(FileSystemUtil.ReadFile(filePath))
    ReadJsonFromFile = jsonString
End Function
Function GetCredentials(jsonFilePath)
    Dim jsonObject
    jsonObject = ReadJsonFromFile(jsonFilePath)
    GetCredentials = jsonObject
End Function
' Include the function library
ExecuteFile "JsonUtils.vbs"

' Call the function to get credentials
jsonFilePath = "path/to/credentials.json"
credentials = GetCredentials(jsonFilePath)

' Use the retrieved credentials
username = credentials("username")
password = credentials("password")

' Use the username and password in your script (e.g., login)
Browser("YourBrowser").Page("YourPage").WebEdit("username").Set username
Browser("YourBrowser").Page("YourPage").WebEdit("password").Set password
