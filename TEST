file_dialog_element = None

# Check if `file_dialog["File name:"]` is iterable (list-like)
if isinstance(file_dialog["File name:"], (list, tuple)):
    # Iterate over the elements
    for element in file_dialog["File name:"]:
        # Check if the element is visible
        if element.is_visible():
            file_dialog_element = element
            break
