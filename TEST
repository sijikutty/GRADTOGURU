' Create an Excel application object
Dim objExcel
Set objExcel = CreateObject("Excel.Application")

' Hide Excel application window (optional)
objExcel.Visible = False

' Open the Excel file
Dim wb
Set wb = objExcel.Workbooks.Open("C:\Path\to\Your\File.xlsx") ' Replace with the path to your Excel file

' Activate the workbook
wb.Activate

' Select cell I45
objExcel.ActiveSheet.Range("I45").Select

' Unhide Excel application window (optional)
' objExcel.Visible = True

' Cleanup: Close the workbook and quit Excel
wb.Close
objExcel.Quit

' Release object references
Set wb = Nothing
Set objExcel = Nothing
