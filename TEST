    value_found = False
    for row in rows.all():
        input_value = row.locator('td[id^="PORTAL_SECDYVW_PORTAL_AUTHNAME"] input').input_value()  # Adjust the selector for the input
        if input_value == 'pp_ww':
            value_found = True
            break

    # Assert that the value was found
    expect(value_found).to_be(True)
