         for idx, view_link in enumerate(view_links):
                    href = view_link.evaluate("this.href")
                    text = view_link.evaluate("this.textContent")
                    id = view_link.evaluate("this.id")
                    name = view_link.evaluate("this.name")
                    print(f"View Log/Trace Link {idx}: href={href}, text={text}, id={id}, name={name}")
                
                # Click the first available link (or choose based on your criteria)
                view_links[0].click()
