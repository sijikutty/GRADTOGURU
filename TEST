' Launch the Edge browser
SystemUtil.Run "msedge.exe"

' Navigate to the desired URL
Browser("iConnect - Home_2").Navigate "http://p2807hldpst001.thehartford.com:8300/psp/fgl2d/?cmd=login&languageCd=ENG&"

' Perform login actions
Browser("iConnect - Home_2").Page("Oracle PeopleSoft Sign-in").WebEdit("userid").Set "PWTEST"
Browser("iConnect - Home_2").Page("Oracle PeopleSoft Sign-in").WebEdit("pwd").Set "Pwtest@100"
Browser("iConnect - Home_2").Page("Oracle PeopleSoft Sign-in").WebButton("To enable screen reader").Click

' Navigate to the desired page
Browser("iConnect - Home_2").Page("Homepage").Image("NavBar").Click
Browser("iConnect - Home_2").Page("Homepage").Frame("psNavBarIFrame").WebButton("PTNB$PTNUI_NB_MENU").Click
Browser("iConnect - Home_2").Page("Homepage").Frame("psNavBarIFrame").Link("Reporting Tools").Click
Browser("iConnect - Home_2").Page("Homepage").Frame("psNavBarIFrame").Link("PS/nVision").Click
Browser("iConnect - Home_2").Page("Homepage").Frame("psNavBarIFrame").Link("PS/nVision Schedule Books").Click

' Set the value in the search field
Browser("iConnect - Home_2").Page("PS/nVision Schedule Books").Frame("TargetContent").WebEdit("PSNVSBOOK_RUN_CNTL_ID").Set "test7"

' Click the search button
Browser("iConnect - Home_2").Page("PS/nVision Schedule Books").Frame("TargetContent").WebButton("Search").Click

' Check if the text exists on the page
Dim textExists
textExists =Browser("iConnect - Home_2").Page("PS/nVision Schedule Books").Frame("TargetContent").WebElement("PTS_CRITTEXT_DISP").CheckProperty("innertext","No matching values were found.")
