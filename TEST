' Open File Explorer and navigate to the specified directory
SystemUtil.Run "explorer.exe", "C:\Users", "", "", 1

' Wait for File Explorer to open (you might need to adjust the sleep time)
Wait(2)

' Open the Excel file in the specified directory
SystemUtil.Run "excel.exe", "C:\Users\vv.xlsm", "", "", 1

' Wait for Excel to open (you might need to adjust the sleep time)
Wait(2)
