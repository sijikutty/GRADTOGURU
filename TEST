Function WriteRallyJson(TCName, TCID, verdict, result)
    ' Your existing code...
    
    ' Construct the command string
    Dim command
    command = """" & pythonExePath & """ """ & scriptPath & """ """ & jsonData & """ """ & s3bucket & """ """ & s3filename & """"
    
    ' Execute the command using SystemUtil.Run
    Dim output
    SystemUtil.Run command, 1, True, output
    
    ' Check if any error occurred within the output
    If InStr(output, "Traceback (most recent call last):") > 0 Then
        MsgBox "Python script failed with error:" & vbCrLf & output
    Else
        MsgBox "Python script executed successfully."
    End If
    
    ' Your existing code...
End Function
