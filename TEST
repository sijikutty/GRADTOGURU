' Create an Excel application object
Dim objExcel
Set objExcel = CreateObject("Excel.Application")

' Hide Excel application window (optional)
objExcel.Visible = True

' Open the Excel file with add-ins enabled
Dim wb
Set wb = objExcel.Workbooks.Open(downloadsFolderPath & "\" & fileName, , False, , , , , , , True)

' Activate the workbook
wb.Activate

' Select cell I45 in the active worksheet
objExcel.ActiveSheet.Range("I45").Select

' Release object references
Set wb = Nothing
Set objExcel = Nothing
