    connection = connect_to_oracleDB(testdata["ps_db"])
    
    # Query to count records for the given process instance


    query = "SELECT TO_CHAR(MAX(EFFDT), 'MM/DD/YYYY') FROM PS_CURR_STEP_TBL WHERE CURR_STEP='"+testdata['Group']+"' AND EFF_STATUS='A'"
    print(query)  
    
    if connection:
        # Execute the query and retrieve the result
        query_result = execute_query(connection, query)

        # Check the query execution status and print the results
        if query_result["status"]:
            if query_result['count'] !='' :
                print(f"Query executed successfully. Date: {query_result['count']}")
                As_of_Date=query_result['count']
                As_of_Date=As_of_Date+timedelta(days=2)
            else:
                raise Exception(f"Query Result has no rows")
        else:
            raise Exception(f"Failed to execute query. Error: {query_result['error']}")
    else:
        raise Exception("Failed to connect to the database.")

ERROR can only concatenate str (not "datetime.timedelta") to str
