view_link_selector = "a"
 link_locator = frame.locator(view_link_selector).locator("text=View Log/Trace")
                if link_locator.count() > 0:
                    link_locator.first.click()  # Click the first matching link
                    print("Clicked the first 'View Log/Trace' link inside the iframe.")
                else:
                    print("No link with the text 'View Log/Trace' found inside the iframe.")
