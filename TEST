jsonData = "{"
jsonData = jsonData & """key"":""" & jsonContent("key") & ""","
jsonData = jsonData & """Rally"":{"
For Each key In rallyContent.Keys
    jsonData = jsonData & """" & key & """:""" & rallyContent(key) & ""","
Next
jsonData = Left(jsonData, Len(jsonData) - 1) ' Remove the trailing comma
jsonData = jsonData & "}}"
