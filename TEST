' Get a reference to the Excel window
Set excelWindow = Window("nativeclass:=XLMAIN")

If excelWindow.Exist(10) Then
    ' Retrieve the window title and class name
    windowTitle = excelWindow.GetROProperty("title")
    className = excelWindow.GetROProperty("nativeclass")

    ' Display the window title and class name in a message box
    MsgBox "Excel Window Title: " & windowTitle & vbCrLf & "Class Name: " & className
Else
    MsgBox "Excel window not found."
End If
