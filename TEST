    targetContentFrame.get_by_label("Output and Journal Options").click()


    # Get the row corresponding to the required program to run and check the checkbox
    sample_bookcd_row = targetContentFrame.locator("tr[id^='tr']").filter(has_text='Book Code')
    
    #sample_bookcd_row.get_by_label("Retain Value").uncheck()
    sample_bookcd_row.get_by_role("checkbox").uncheck()
    sample_bookcd_row.get_by_role("textbox").fill("STNC")

    targetContentFrame.get_by_role("button", name="Save").click()
