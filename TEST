import os

def get_latest_file_from_folder(folder_path):
    files = [os.path.join(folder_path, f) for f in os.listdir(folder_path) if os.path.isfile(os.path.join(folder_path, f))]
    if files:
        latest_file = max(files, key=os.path.getctime)
        return latest_file
    else:
        return None

# Replace 'YOUR_DOWNLOAD_FOLDER_PATH' with the path to your download folder
download_folder = 'YOUR_DOWNLOAD_FOLDER_PATH'
latest_file = get_latest_file_from_folder(download_folder)
if latest_file:
    print("Latest file in download folder:", latest_file)
else:
    print("No files found in download folder.")
