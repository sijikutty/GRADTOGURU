import boto3
import sys

def upload_to_s3(file_path, bucket_name, s3_file_path):
    # Initialize S3 client
    s3 = boto3.client('s3')

    # Upload file to S3 bucket
    with open(file_path, 'rb') as file:
        s3.upload_fileobj(file, bucket_name, s3_file_path)

if __name__ == "__main__":
    # Get file path, bucket name, and S3 file path from command line arguments
    file_path = sys.argv[1]
    bucket_name = sys.argv[2]
    s3_file_path = sys.argv[3]

    # Upload file to S3
    upload_to_s3(file_path, bucket_name, s3_file_path)
------------
Dim file_path, bucket_name, s3_file_path, python_script_path
file_path = "C:\path\to\your\file.txt"  ' Set the local file path
bucket_name = "your-bucket-name"         ' Set the S3 bucket name
s3_file_path = "path/to/your/s3/file.txt" ' Set the S3 file path
python_script_path = "C:\path\to\your\python_script.py"  ' Set the path to your Python script

' Execute the Python script using SystemUtil.Run
SystemUtil.Run "python", """" & python_script_path & """ """ & file_path & """ """ & bucket_name & """ """ & s3_file_path & """", "", output
