    frame.get_by_role("button", name="Refresh").click()
    expect(frame.locator("[id=\"nestedDetailPageSuffix\\:logsTable\"]")).to_contain_text("RUNNING")
    frame.get_by_role("button", name="Refresh").click()

    expect(page.frame_locator("#QuickView").locator("#jobResultTable")).to_contain_text("37902191")
    expect(page.frame_locator("#QuickView").locator("[id=\"nestedDetailPageSuffix\\:logsTable\"]")).to_contain_text("SUCCESS")
    
    process_inst_row = frame.locator("tr[id^='tr']").filter(has_text=process_inst_id)
    run_status = process_inst_row.locator("[id^='PMN_PRCSLIST_RUNSTATUSDESCR']")
