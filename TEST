import win32com, win32com.client

uft = win32com.client.Dispatch("QuickTest.Application")

# starts up uft
uft.Launch() 

# make the uft window visible
uft.Visible = True

# Open a test, replace the path 
uft.Open(r"C:\Github\corp_corpit_ps_egl_test_automation\UFT\FAH Concurrent Program")

# to open a uft test in Quality Center
# uft.Open(r"[QualityCenter] Subject\FolderName\uftScript")

# create a RunResultsOptions object
qtResultsOpt = win32com.client.Dispatch("QuickTest.RunResultsOptions")

# set the location to where the results will be save
qtResultsOpt.ResultsLocation = "C:\Github\corp_corpit_ps_egl_test_automation\UFT\FAH Concurrent Program\res"

uft.Test.Run(qtResultsOpt)

#print "Test has %s" %uft.Test.LastRunResults.Status

# close the Test
uft.Test.Close()

# quit uft
uft.Quit()
