Dim jsonFilePath
jsonFilePath = "path_to_your_json_file.json" ' Replace this with the actual path to your JSON file

' Read the JSON file contents into a string
Dim jsonContents
Set jsonContents = CreateObject("Scripting.FileSystemObject").OpenTextFile(jsonFilePath).ReadAll()

' Parse the JSON string using JsonUtil.Parse
Dim parsedJson
Set parsedJson = JsonUtil.Parse(jsonContents)

' Now you can access the JSON data using parsedJson
' For example, if your JSON file contains an object with key "exampleKey", you can access its value like this:
Dim exampleValue
exampleValue = parsedJson("exampleKey")

' Use exampleValue as needed
