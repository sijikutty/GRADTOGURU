' Define the path to the Python interpreter and your Python script
python_path = "C:\Python39\python.exe" ' Adjust the path to your Python interpreter
script_path = "C:\path\to\your\python_script.py" ' Adjust the path to your Python script

' Run the Python script
Set shell = CreateObject("WScript.Shell")
command = """" & python_path & """ """ & script_path & """"
shell.Run command, 0, True ' Run synchronously and wait for completion

' Check for errors
On Error Resume Next
If Err.Number <> 0 Then
    MsgBox "Error: Python script failed to execute. Error number: " & Err.Number & vbCrLf & "Error description: " & Err.Description
End If
On Error GoTo 0
