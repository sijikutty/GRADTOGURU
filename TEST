import win32com.client

# Create a QTP/UFT object
qtp = win32com.client.Dispatch("QuickTest.Application")

# Start QTP/UFT
qtp.Launch()

# Make QTP/UFT window visible
qtp.Visible = True

# Open a test (replace the path with your test path)
test_path = r"C:\path\to\your\test"
qtp.Open(test_path)

# Define parameters
parameters = {"param1": "value1", "param2": "value2"}

# Run the test with parameters
qtp.Test.Run(parameters)
------
' VBScript code within UFT test
param1_value = Parameter("param1").Value
param2_value = Parameter("param2").Value

' Use the parameter values in your test script
