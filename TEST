 # Wait for the button to be visible
    page.wait_for_selector('input#ICSave[value="OK"]')  # You can also use other attributes if needed

    # Locate the "OK" button and click it
    ok_button = page.locator('input#ICSave[value="OK"]')
    ok_button.click()

