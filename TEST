from playwright.sync_api import sync_playwright

def get_first_row_text():
    with sync_playwright() as p:
        browser = p.chromium.launch()
        page = browser.new_page()
        
        # Navigate to your page or perform necessary actions to reach the frame
        page.goto("https://example.com")
        
        # Wait for the frame to load
        frame = page.frame_locator("#QuickView").locator("[id=\"nestedDetailPageSuffix\\:logsTable\"]")
        await frame.wait_for_load_state("networkidle")

        # Get the first row of the table
        first_row_text = await frame.eval_on_selector("tbody tr:first-child td", "element => element.textContent")
        
        print("Text from the first row:", first_row_text.strip())

        await browser.close()

# Run the function to fetch the first row text
get_first_row_text()
