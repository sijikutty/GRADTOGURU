page.frame_locator("iframe[name=\"ptModFrame_2\"]").get_by_label("Server Name").select_option("PSUNX")
    page.frame_locator("iframe[name=\"ptModFrame_2\"]").locator("[id=\"PRCSRQSTDLG_WRK_SELECT_FLAG\\$0\"]").uncheck()
    page.frame_locator("iframe[name=\"ptModFrame_2\"]").locator("[id=\"PRCSRQSTDLG_WRK_SELECT_FLAG\\$0\"]").check()
    page.frame_locator("iframe[name=\"ptModFrame_2\"]").get_by_role("button", name="OK", exact=True).click()
