# Fetch the id of the iframe with the title "Process Detail"
iframe_id = page.query_selector("iframe").content_frame().evaluate("document.querySelector('iframe[title=\"Process Detail\"]').id")

# Use that id to create a frame locator and interact with the iframe
popup_dialog = page.frame_locator(f"iframe#{iframe_id}")
popup_dialog.get_by_role("link", name="View Log/Trace").click()
