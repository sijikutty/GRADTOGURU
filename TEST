    # Locate the table rows
    rows = page.frame_locator('iframe[name="TargetContent"]').locator('table tbody tr')

    # Loop through each row and check if it contains the desired text
    value_found = False
    for row in rows.all():
        cell_text = row.locator('td[id^="PORTAL_SECDYVW_PORTAL_AUTHNAME"]').inner_text()
        if 'pp_ww' in cell_text:
            value_found = True
            break

    # Assert that the value was found
    expect(value_found).to_be(True)
