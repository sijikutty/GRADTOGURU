' Create Edge Browser Object
Set browser = Browser("Edge")

' Launch Edge Browser
SystemUtil.Run "msedge.exe", "http://p2807hldpst001.thehartford.com:8300/psp/fgl2d/?cmd=login&languageCd=ENG"

' Wait for the browser to open (adjust wait time as needed)
Wait 5 ' Adjust wait time as needed

' Get the Browser object
Set page = browser.Page("Page")

' Perform Login Action
page.WebEdit("userid").Set "PWTEST" ' Enter username
page.WebEdit("pswd").Set "Pwtest@100" ' Enter password

' Click on Login Button
page.WebButton("Login").Click ' Assuming there is a button with the label "Login" to perform login action

' Wait for some time for the login process to complete
Wait 10 ' Adjust wait time as needed

' Check for login success or failure
If page.Exists("Some_Element_On_Next_Page") Then
    MsgBox "Login Successful."
Else
    MsgBox "Login Failed."
End If

' Close the browser
browser.Close
