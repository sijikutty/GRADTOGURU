Dim oShell, cmd, pythonExePath

Set oShell = CreateObject("WScript.Shell")

' Command to get the path of python.exe
cmd = "where python"

' Execute the command and capture the output
pythonExePath = oShell.Exec(cmd).StdOut.ReadAll()

' Remove any leading or trailing whitespace or line breaks
pythonExePath = Trim(pythonExePath)

If pythonExePath <> "" Then
    MsgBox "Python executable found at: " & pythonExePath
Else
    MsgBox "Python executable not found."
End If

Set oShell = Nothing
