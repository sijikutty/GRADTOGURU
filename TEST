# Assuming testdata is defined as in your JSON
components = [
    testdata['component1'],
    testdata['component2'],
    testdata['component3'],
    testdata['component4'],
    testdata['component5'],
    testdata['component6'],
    testdata['component7'],
    testdata['component8'],
    testdata['component9'],
    testdata['component10'],
]

# Select the row based on menu
sample_select_row = frame.locator("tr[id^='tr']").filter(has_text=testdata['menu'])
sample_select_row.get_by_text("Edit Components").click()

# Loop through each component and check if it's selected
for component in components:
    component_locator = frame.locator("tr[id^='tr']").filter(has_text=component)
    checkbox = component_locator.get_by_role("checkbox")

    if checkbox.is_checked:
        print(f"{component} has been selected properly")
    else:
        raise Exception(f"{component} is not selected properly")
