Function WriteToUFTOne(result)
    If result = "Test is success" Then
    	AIUtil("button", "OK").Click
    	Set jrnlData=fetch_jrnlid()
    	' Extract the values from the dictionary
        journal_id = jrnlData("Journal_id")
        business_unit = jrnlData("Business_unit")
        Reporter.ReportEvent micPass, "Test Result", result
        verdict="Pass"
        JournalDataJson business_unit,journal_id
    Else
        Reporter.ReportEvent micFail, "Test Result", result
        verdict="Fail"
    End If
        WriteRallyJson "Smoketest for Load Journal to Template","TC32541",verdict,result
End Function
