 # Get the iframe element
        iframe_element = page.locator('iframe[title="process detail"]')

        # Access the inner frame
        iframe = iframe_element.content_frame()

        # Wait for the 'view log/trace' button to be visible
        iframe.wait_for_selector('text="view log/trace"', timeout=5000)

        # Get the 'view log/trace' element
        log_trace_element = iframe.locator('text="view log/trace"')

        # Get properties/attributes of the element
        properties = {
            'text': log_trace_element.inner_text(),
            'href': log_trace_element.get_attribute('href'),  # Example for link elements
            'class': log_trace_element.get_attribute('class'),
            'id': log_trace_element.get_attribute('id'),
            # Add more attributes as necessary
        }

        # Print the properties
        print("Properties of 'view log/trace':")
        for key, value in properties.items():
            print(f"{key}: {value}")
