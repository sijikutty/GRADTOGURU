# Get the first row of the table
            first_row = frame.locator("tbody tr:first-child")
            
            # Get the text from the first cell in the first row
            first_cell = first_row.locator("td").nth(0)  # Select the first cell
            first_cell_text = first_cell.inner_text().strip()

            print("Text from the first cell in the first row:", first_cell_text)
        
