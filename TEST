' Open Excel with the specific file (vv.xlsm)
objShell.Run "excel.exe """ & downloadsFolderPath & "\" & fileName & """", 1, True

' Wait for Excel to open
Wait 2 ' Adjust the wait time as needed

' Get a reference to the Excel application
Dim objExcel
Set objExcel = GetObject(, "Excel.Application")

' Make Excel visible
objExcel.Visible = True

' Select cell I45 in the active worksheet
objExcel.ActiveSheet.Range("I45").Select

' Activate the Excel window by making it the foreground window
objShell.AppActivate "Microsoft Excel"

' Access the Excel Ribbon
Set ribbon = objExcel.CommandBars("Ribbon")

' Select the "Add-ins" tab in the Ribbon
ribbon.FindControl(, , "TabAddIns").Execute

' Access the Lower Ribbon
Set lowerRibbon = objExcel.CommandBars("Ribbon")

' Click the "nVisionDrill" button in the Lower Ribbon
lowerRibbon.FindControl(, , "nVisionDrill").Execute

' Access the Ribbon again to access the WinMenu
Set ribbon = objExcel.CommandBars("Ribbon")

' Select the "Drill" option from the WinMenu
ribbon.FindControl(, , "Drill").Execute

' Release object references
Set objExcel = Nothing
