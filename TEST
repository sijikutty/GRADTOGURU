    page.frame_locator("iframe[name=\"ptModFrame_2\"]").get_by_label("Server Name").select_option("PSUNX")
    expect(page.frame_locator("iframe[name=\"ptModFrame_2\"]").get_by_role("button", name="OK", exact=True)).to_be_visible()
    page.frame_locator("iframe[name=\"ptModFrame_2\"]").get_by_role("button", name="OK", exact=True).click()

    page.frame_locator("iframe[name=\"ptModFrame_5\"]").get_by_label("Server Name").select_option("PSUNX")
    expect(page.frame_locator("iframe[name=\"ptModFrame_5\"]").get_by_role("button", name="OK", exact=True)).to_be_visible()
    page.frame_locator("iframe[name=\"ptModFrame_5\"]").get_by_role("button", name="OK", exact=True).click()

    page.frame_locator("iframe[name=\"ptModFrame_6\"]").get_by_label("Server Name").select_option("PSUNX")
    expect(page.frame_locator("iframe[name=\"ptModFrame_6\"]").get_by_role("button", name="OK", exact=True)).to_be_visible()
    page.frame_locator("iframe[name=\"ptModFrame_6\"]").get_by_role("button", name="OK", exact=True).click()
