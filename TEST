link_text = 'More information...'  # Replace with the link text you want to find
        link_exists = page.locator(f'//div[contains(@class, "popup-selector")]//a[text()="{link_text}"]').count()

        if link_exists > 0:
            print('Link is present in the popup.')
        else:
            print('Link is not present in the popup.')
