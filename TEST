' Get a reference to the Excel window
Set excelWindow = Window("regexpwndtitle:=Microsoft Excel.*")

If excelWindow.Exist(10) Then
    ' Use descriptive programming to identify the Ribbon object
    Set ribbon = excelWindow.WinObject("regexpwndclass:=Ribbon.*")

    If Not ribbon.Exist(10) Then
        MsgBox "Ribbon object not found."
        Exit ' Exit the script block
    End If

    ' Use descriptive programming to identify the Ribbon Tabs
    Set ribbonTabs = ribbon.WinTab("regexpwndclass:=Ribbon Tabs.*")

    If Not ribbonTabs.Exist(10) Then
        MsgBox "Ribbon Tabs object not found."
        Exit ' Exit the script block
    End If

    ' Select "Add-ins" tab in Ribbon Tabs
    ribbonTabs.Select "Add-ins"
Else
    MsgBox "Excel window not found."
End If
