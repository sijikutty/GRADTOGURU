' Parse the JSON string
Dim parsedJson
Set parsedJson = JsonUtil.Parse(jsonContents)

' Debugging: Print the structure of the parsed JSON object
PrintJsonStructure parsedJson

Sub PrintJsonStructure(obj, Optional depth)
    If IsObject(obj) Then
        Dim key
        For Each key In obj
            Dim value
            value = obj(key)
            Dim indent
            indent = String(depth * 2, " ")
            MsgBox indent & key & ": " & TypeName(value)
            If IsObject(value) Then
                PrintJsonStructure value, depth + 1
            End If
        Next
    End If
End Sub
