Dim pythonScriptPath
pythonScriptPath = "C:\path\to\vv.py"

Dim pythonFunctionName
pythonFunctionName = "testcase"

Dim pythonArgs
pythonArgs = "arg1, arg2"  ' Provide any arguments as needed

' Create Python runtime object
Set python = CreateObject("Python.Runtime")

' Call the Python function
python.RunPythonScript pythonScriptPath, pythonFunctionName, pythonArgs
