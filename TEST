import win32com.client

# Create an instance of the UFT application
qtApp = win32com.client.Dispatch("QuickTest.Application")

# Start UFT One
qtApp.Launch()

# Make the UFT One application visible
qtApp.Visible = True

# Open the test
qtApp.Open("D:\\Tests\\MyTest")

# Retrieve the parameters collection defined for the test
pDefColl = qtApp.Test.ParameterDefinitions

# Display the names and values of each of the parameters in the collection
for Indx in range(1, pDefColl.Count + 1):
    pDef = pDefColl.Item(Indx)
    print("Param name:", pDef.Name)
    print("Type:", pDef.Type)
    print("InOut:", pDef.InOut)
    print("Description:", pDef.Description)
    print("Default value:", pDef.DefaultValue)

# Retrieve the Parameters collection defined for the test
rtParams = pDefColl.GetParameters()

# Retrieve a specific parameter
rtParam = rtParams.Item("InParam1")

# Change the parameter value
rtParam.Value = "Hello"

# Run the test with changed parameters
qtApp.Test.Run(True, True, rtParams)

# Display the value of an output parameter after the test runs
print(rtParams.Item("OutParam1").Value)
