if [ $? -eq 0 ]; then
    exit 0
else
    EMAIL=`cat $FIT_SCRIPT/fah_maillist.txt`
    echo "Autosys FAH Processing failed for $1" |mutt -s "Autosys FAH Processing failed for $1" $EMAIL
