import win32com.client

# Path to the Excel file
excel_file_path = "path/to/your/excel/file.xlsx"

# Name of the macro button
macro_button_name = "YourMacroButtonName"

# Create an instance of Excel application
excel_app = win32com.client.Dispatch("Excel.Application")

# Open the Excel file
wb = excel_app.Workbooks.Open(excel_file_path)

# Specify the sheet name if needed
# sheet = wb.Sheets("Sheet1")

# Find the macro button by its name
macro_button = wb.Application.ActiveSheet.Buttons(macro_button_name)

# Click on the macro button
macro_button.Click()

# Close the Excel file
wb.Close()
