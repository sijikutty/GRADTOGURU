frame.evaluate(f"""
                        var element = document.querySelector('{view_link_selector}');
                        if (element) {
                            element.scrollIntoView();
                            // Use setTimeout to ensure click event is registered
                            setTimeout(function() {{
                                element.click();
                            }}, 100);
                        } else {
                            console.error('Element not found');
                        }
                    """)
