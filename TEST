Set WshShell = CreateObject("WScript.Shell")

' Launch the Python script and capture the process ID
pid = WshShell.Run("cmd.exe /c python dynamic_API.py", 0, True) ' 0 means hide the window, True means wait for completion

' Wait for the Python script to finish execution (adjust the sleep time as needed)
WScript.Sleep(10000) ' Wait for 10 seconds (10000 milliseconds)

' Check if the Python script executed successfully
If pid = 0 Then
    MsgBox "Python script executed successfully."
Else
    MsgBox "Error: Python script failed to execute. Return code: " & pid
End If
