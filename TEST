    ' Ensure that the JSON content is not empty
    If Trim(jsonContents) <> "" Then
        Set parsedJson = JsonUtil.Parse(jsonContents)
        If Not IsObject(parsedJson) Then
            MsgBox "Error: Invalid JSON content in the file."
            Exit Function
        End If
        Set ReadJsonFromFile = parsedJson
    Else
        MsgBox "Error: JSON file is empty."
        Exit Function
    End If
