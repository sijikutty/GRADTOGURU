Function OpenLog(LogFileName As String) As Workbook
Dim PriorActiveWB As Workbook
Dim TemplateFileName As String
Dim TemplateDir As String
Dim LogFileSpec As String
    
    TemplateFileName = "Import Log"
    
    Set PriorActiveWB = ActiveWorkbook
    TemplateDir = ActiveWorkbook.Path
    If Not WorkbookIsOpen(LogFileName) Then
        LogFileSpec = TemplateDir + "\" + LogFileName
        If Dir(LogFileSpec) = "" Then
            Workbooks.Add
            ActiveSheet.Name = "Import Log"
            ActiveSheet.Cells(1, 1) = "Workbook"
