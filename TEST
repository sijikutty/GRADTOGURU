' Wait for Excel to open
Wait(5)

' Get the Excel Application object
Set objExcel = GetObject(,"Excel.Application")

' If Excel is not found, exit the script
If objExcel Is Nothing Then
    MsgBox "Excel is not open.", vbExclamation
    Exit Test
End If

' Get the active workbook
Set objWorkbook = objExcel.ActiveWorkbook

' If no workbook is open, exit the script
If objWorkbook Is Nothing Then
    MsgBox "No workbook is open in Excel.", vbExclamation
    Exit Test
End If

' Get the active worksheet
Set objWorksheet = objWorkbook.ActiveSheet

' Get the value of cell W13
currentValue = objWorksheet.Cells(13, 23).Value

' Add 1 to the current value
newValue = currentValue + 1

' Set the new value to cell W13
objWorksheet.Cells(13, 23).Value = newValue

' Save the workbook
objWorkbook.Save

' Close Excel
objExcel.Quit
