Dim objShell
Set objShell = CreateObject("WScript.Shell")

' Path to the Python executable
pythonExe = "C:\Path\to\python.exe"

' Path to the Python script
pythonScript = "C:\Path\to\get_secrets.py"

' Secret ID to fetch (adjust as needed)
secretID = "your-secret-id"

' Command to execute the Python script with arguments
command = pythonExe & " " & pythonScript & " get_secrets " & secretID

' Execute the command
Set objExec = objShell.Exec(command)

' Read the output (if any)
output = objExec.StdOut.ReadAll()

' Display output (optional)
MsgBox output
