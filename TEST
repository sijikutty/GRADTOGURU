if iframe_index is not None:
        # Access the iframe's content frame
        iframe_element = iframes[iframe_index]
        frame = iframe_element.content_frame()

        if frame is not None:
            # Define the selector for the 'View Log/Trace' link
            view_link_selector = "div[id='win0divPMN_DERIVED_INDEX_BTN'] span[title='Index Page'] a"
            
            # Get the outer HTML of the element inside the iframe (for debugging)
            outer_html = frame.evaluate(f'document.querySelector("{view_link_selector}").outerHTML')
            print(f"View Link Outer HTML: {outer_html}")
            
            try:
                # Query and click the element inside the iframe
                element = frame.query_selector(view_link_selector)
                if element is not None:
                    element.click()
                    print(f"Clicked on the element with selector '{view_link_selector}'.")
                else:
                    print(f"Element with selector '{view_link_selector}' not found inside the iframe.")
            except Exception as e:
                print(f"An error occurred: {e}")
        else:
            print("Failed to get content frame from the iframe.")
    else:
        print("No iframe with the specified content title found.")
