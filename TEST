def print_iframe_titles(page: Page):
    # Locate all iframe elements on the page
    iframes = page.locator('iframe')
    
    # Get the count of iframes
    iframe_count = iframes.count()
    
    # Iterate through each iframe
    for index in range(iframe_count):
        iframe_locator = iframes.nth(index)
        try:
            # Get the title attribute of the iframe
            title = iframe_locator.get_attribute('title')
            print(f"Title of iframe {index}: {title}")
        except Exception as e:
            print(f"Could not retrieve title from iframe {index}: {str(e)}")

# Call the function in your test
print_iframe_titles(page)
